  "private": true,
  "repository": "github:opengs/uashield",
  "scripts": {
    "build:headless": "esbuild ./headless.ts --platform=node  --bundle --minify --sourcemap --sources-content=false --keep-names --target=node16 --outfile=./build/headless/headless.js",
