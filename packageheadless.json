    "user-agents": "^1.0.965",
    "uuid4": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.26",
    "@types/user-agents": "^1.0.2",
    "esbuild": "0.14.27"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 8"
  }
}
